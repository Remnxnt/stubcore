int getKey(unsigned char key_out[32]) {

	char proccessorID[256];

#ifdef _WIN32
	DWORD bufSize = sizeof(proccessorID);
	if (!GetSystemFirmwareTable('RSMB', 0, proccessorID, bufSize)) {
		return 0;
	}
#else
	FILE* f = fopen("/sys/class/dmi/id/processor_serial", "r");
	if (!f) return 0;
	fgets(proccessorID, sizeof(proccessorID), f);
	fclose(f);
#endif

	SHA256((unsigned char*)proccessorID
		strlen(proccessorID),
		key_out);

	return 1;
}